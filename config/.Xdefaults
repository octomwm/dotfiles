    !Colors

    ! URxvt.depth: 32
    *foreground: #D7D0C7
    *background: #252525
    URxvt.cursorColor: #FFFFFF
    URxvt.cursorColor2: #252525
    URxvt.colorIT: #444
    URxvt.colorBD: #FFFFFF
    URxvt.colorUL: #FF8F00
    URxvt.colorRV: #333333
    URxvt.underlineColor: #0E0E0E
    URxvt.borderColor: #252525
    URxvt.pointerColor: #000000
    Xterm.pointerColor: #ffffff
    *pointerColor2: #FFFFFF
    *internalBorder: 23

    !  colors. Flashy, monokai inspired
    !black
    *color0: #252525
    *color8: #444444
    !red
    *color1: #E84F4F
    *color9: #D23D3D
    !green
    *color2:  #6DC130
    *color10: #9FE93A
    !yellow
    *color3:  #FFAE26
    *color11: #FFED3C
    !blue
    *color4:  #3C80EA
    *color12: #40A7FF
    !magenta
    *color5:  #DE55E1
    *color13: #953BED
    !cyan
    *color6:  #30B5BF
    *color14: #3AE8CD
    !white
    *color7:  #FFFFFF
    *color15: #878787

