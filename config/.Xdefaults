    !Font

    xterm*faceName: PragmataTT:size=9:antialias=false
    ! xterm*faceName: Droid Sans Mono:size=9:antialias=true
    ! URxvt.letterSpace: -3

    ! URxvt.font: xft:Droid Sans Mono:antialias=false:pixelsize=12, \
    !            xft:unifont:antialias=false
    ! URxvt.boldFont:xft:Droid Sans Mono:antialias=true:style=bold:pixelsize=12, \
    !            xft:unifont:antialias=false:style=bold
    ! URxvt.italicFont:xft:Droid Sans Mono:antialias=true:style=italic:pixelsize=12, \
    !            xft:unifont:antialias=false:style=italic
    URxvt.font: xft:PragmataTT:antialias=false:pixelsize=12, \
               xft:unifont:antialias=false
    URxvt.boldFont:xft:PragmataTT:antialias=false:style=bold:pixelsize=12, \
               xft:unifont:antialias=false:style=bold
    URxvt.italicFont:xft:PragmataTT:antialias=false:style=italic:pixelsize=12, \
               xft:unifont:antialias=false:style=italic

    !Plugin

    URxvt.perl-ext-common:  default,matcher,clipboard
    ! URxvt.perl-ext:  default,clipboard
    URxvt.url-launcher:      /usr/bin/firefox-nightly
    URxvt.matcher.button:   1

    !general

    URxvt.scrollBar: false
    imLocale: fr_CH.UTF-8
    saveLines: 5000
    URxvt.geometry: 95x26+50+50
