    !Font

    ! URxvt.font: xft:PragmataPro:size=7.5

    xterm*faceName: PragmataTT:size=8:antialias=false

    ! Forbid bold font faces; bold type is light blue.
    XTerm*colorBDMode: true
    ! XTerm*colorBD: rgb:82/a4/d3
    ! Do not underscore text, underlined text is white.
    ! XTerm*colorULMode: true
    ! XTerm*colorUL: rgb:e4/e4/e4

    URxvt.font: xft:PragmataTT:antialias=false:pixelsize=11, \
                xft:unifont:antialias=false
    URxvt.boldFont:xft: PragmataTT:antialias=false:style=bold:pixelsize=11, \
                xft:unifont:antialias=false:style=bold

    URxvt.italicFont:xft: PragmataTT:antialias=false:style=italic:pixelsize=11, \
                xft:unifont:antialias=false:style=italic

    !Plugin

    perl-ext-common:  default,matcher,clipboard
    ! URxvt.perl-ext:  default,clipboard
    url-launcher:      /usr/bin/firefox-aurora
    URxvt.matcher.button:   1

    !general

    URxvt.scrollBar: false
    imLocale: fr_CH.UTF-8
    saveLines: 5000
    URxvt.geometry: 95x26+50+50
